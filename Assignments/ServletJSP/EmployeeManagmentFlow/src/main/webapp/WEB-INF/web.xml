<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginController</servlet-name>
  	<display-name>LoginController</display-name>
  	<description></description>
  	<servlet-class>org.employee.controller.LoginController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EmployeeListServlet</servlet-name>
  	<display-name>EmployeeListServlet</display-name>
  	<description></description>
  	<servlet-class>org.employee.controller.EmployeeListServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LogoutServlet</servlet-name>
  	<display-name>LogoutServlet</display-name>
  	<description></description>
  	<servlet-class>org.employee.controller.LogoutServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RegistrationServlet</servlet-name>
  	<display-name>RegistrationServlet</display-name>
  	<description></description>
  	<servlet-class>org.employee.controller.RegistrationServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EmployeeAddServlet</servlet-name>
  	<display-name>EmployeeAddServlet</display-name>
  	<description></description>
  	<servlet-class>org.employee.controller.EmployeeAddServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FindEmployeeServlet</servlet-name>
  	<display-name>FindEmployeeServlet</display-name>
  	<description></description>
  	<servlet-class>org.employee.controller.FindEmployeeServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateEmployeeServlet</servlet-name>
  	<display-name>UpdateEmployeeServlet</display-name>
  	<description></description>
  	<servlet-class>org.employee.controller.UpdateEmployeeServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteEmployeeServlet</servlet-name>
  	<display-name>DeleteEmployeeServlet</display-name>
  	<description></description>
  	<servlet-class>org.employee.controller.DeleteEmployeeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginController</servlet-name>
  	<url-pattern>/login.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EmployeeListServlet</servlet-name>
  	<url-pattern>/EmployeeList.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LogoutServlet</servlet-name>
  	<url-pattern>/logout.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegistrationServlet</servlet-name>
  	<url-pattern>/register.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EmployeeAddServlet</servlet-name>
  	<url-pattern>/EmployeeAdd.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FindEmployeeServlet</servlet-name>
  	<url-pattern>/FindEmployee.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateEmployeeServlet</servlet-name>
  	<url-pattern>/UpdateEmployee.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteEmployeeServlet</servlet-name>
  	<url-pattern>/delete.do</url-pattern>
  </servlet-mapping>
</web-app>
